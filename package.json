{
  "name": "notebouque",
  "version": "0.1.0",
  "description": "Note taking for developers",
  "main": "main.js",
  "scripts": {
    "start": "ENV=development concurrently -k 'babel-node server.js' 'electron .'",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:js": "eslint ./",
    "lint:css": "stylelint ./src/**/*.css",
    "test": "npm run lint && BABEL_DISABLE_CACHE=1 NODE_ENV=test NODE_PATH='src/js' istanbul cover --colors  _mocha -- --require './test/setup.js' --compilers js:babel-register --recursive 'src/**/*.test.js'",
    "test:watch": "BABEL_DISABLE_CACHE=1 NODE_ENV=test NODE_PATH='src/js' mocha 'src/js/**/*.test.js' --recursive --require './test/setup.js' --compilers js:babel-register --watch"
  },
  "keywords": [],
  "author": "Daniel Sneijers",
  "license": "ISC",
  "moduleRoots": [
    "src/js"
  ],
  "dependencies": {
    "babel-core": "^6.17.0",
    "classnames": "^2.2.5",
    "highlight.js": "^9.7.0",
    "marked": "^0.3.6",
    "moment": "^2.15.1",
    "react": "^15.3.0",
    "react-autosize-textarea": "^0.3.2",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "stylelint": "^7.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "electron": "^1.4.4",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.0",
    "express": "^4.14.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.0",
    "postcss": "^5.2.5",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^1.0.0",
    "react-addons-test-utils": "^15.3.0",
    "redux-logger": "^2.6.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.4.2",
    "stylelint-config-standard": "^13.0.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
