{
  "name": "dev-note",
  "version": "0.1.0",
  "description": "Note taking for developers",
  "main": "main.js",
  "scripts": {
    "start": "ENV=development concurrently -k 'babel-node server.js' 'electron .'",
    "test": "ENV=test NODE_PATH='src/js' istanbul cover --colors  _mocha -- --require './config/testSetup.js' --compilers js:babel-register --recursive 'src/**/*.test.js'",
    "test:watch": "ENV=test NODE_PATH='src/js' mocha 'src/js/**/*.test.js' --recursive --harmony_proxies --require './config/testSetup.js' --watch",
    "plato": "plato -r -d report src"
  },
  "keywords": [],
  "author": "Daniel Sneijers",
  "license": "ISC",
